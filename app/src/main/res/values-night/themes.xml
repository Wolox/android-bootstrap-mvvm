<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Androidbootstrap" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/wolox_blue</item>
        <item name="colorPrimaryVariant">@color/wolox_dark_blue</item>
        <item name="colorOnPrimary">@color/wolox_white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/wolox_green</item>
        <item name="colorSecondaryVariant">@color/wolox_yellow</item>
        <item name="colorOnSecondary">@color/wolox_black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>
